{
  "version": 2,
  "builds": [
    {
      "src": "/api/chat.js",
      "use": "@vercel/node"
    },
    {
      "src": "/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "/assets/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}