{
    "version": 2,
    "builds": [
        {
            "src": "api/chat.js",
            "use": "@vercel/node"
        },
        {
            "src": "assets/**/*",
            "use": "@vercel/static"
        },
        {
            "src": "*.html",
            "use": "@vercel/static"
        },
        {
            "src": "*.js",
            "use": "@vercel/static"
        },
        {
            "src": "*.css",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/chat",
            "dest": "/api/chat.js"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}